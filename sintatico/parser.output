Grammar

    0 $accept: program $end

    1 program: line_elems

    2 line_elems: line_elem line_elems
    3           | line_elem

    4 line_elem: fundecl
    5          | procdecl
    6          | funtype_decl

    7 fundecl: ID args '=' expr ';'
    8        | ID args '=' expr where_exp
    9        | ID '=' expr ';'
   10        | ID '=' expr where_exp
   11        | ID args '=' error ';'
   12        | ID error '=' expr ';'

   13 args: arg_value args
   14     | UNDERSCORE args
   15     | arg_value
   16     | UNDERSCORE

   17 arg_value: '(' list_value ')'
   18          | basic_value
   19          | '(' arg_value ')'

   20 id_list: ID ':' id_list
   21        | UNDERSCORE ':' id_list
   22        | ID
   23        | UNDERSCORE

   24 basic_value: NUMBER
   25            | FLOATNUM
   26            | BOOLVAL
   27            | ID
   28            | '(' ')'

   29 list_value: basic_value ':' list_value
   30           | '[' list_args ']' ':' list_value
   31           | UNDERSCORE ':' UNDERSCORE
   32           | UNDERSCORE ':' list_value
   33           | built_list_value
   34           | basic_value ':' ID
   35           | basic_value ':' UNDERSCORE

   36 list_args: arg_value
   37          | arg_value ',' list_args

   38 built_list_value: '[' ']'
   39                 | '[' list_args ']'

   40 funtype_decl: ID DOUBLECOLON funtype ';'

   41 funtype: basic_type
   42        | '(' funtype ')'
   43        | basic_type RARROW funtype
   44        | '(' funtype ')' RARROW funtype

   45 basic_type: INTEGER
   46           | FLOAT
   47           | BOOL
   48           | '[' basic_type ']'
   49           | ID
   50           | '(' ')'

   51 appexpr: appexpr nonapp
   52        | nonapp nonapp

   53 nonapp: basic_value
   54       | '(' expr ')'

   55 expr: op_prec1
   56     | appexpr
   57     | ifexpr

   58 ifexpr: IF expr THEN expr ELSE expr

   59 yieldexpr: YIELD ifexpr
   60          | YIELD appexpr
   61          | YIELD op_prec1

   62 op_prec1: op_prec2 OR op_prec1
   63         | op_prec2

   64 op_prec2: op_prec3 AND op_prec2
   65         | op_prec3

   66 op_prec3: op_prec4 EQUALS op_prec3
   67         | op_prec4 DIFF op_prec3
   68         | op_prec4 '<' op_prec3
   69         | op_prec4 LEQ op_prec3
   70         | op_prec4 '>' op_prec3
   71         | op_prec4 GEQ op_prec3
   72         | op_prec4

   73 op_prec4: op_prec5 ':' op_prec4
   74         | op_prec5 APPEND op_prec4
   75         | op_prec5

   76 op_prec5: op_prec5 '+' op_prec6
   77         | op_prec5 '-' op_prec6
   78         | op_prec6

   79 op_prec6: op_prec6 '%' op_prec7
   80         | op_prec7

   81 op_prec7: op_prec7 '*' op_prec8
   82         | op_prec7 '/' op_prec8
   83         | op_prec8

   84 op_prec8: basic_value
   85         | list_expr
   86         | '(' expr ')'
   87         | '-' op_prec8

   88 exprs: expr
   89      | expr ',' exprs

   90 list_expr: '[' exprs ']'
   91          | '[' ']'

   92 where_exp: WHERE '{' line_elems '}'

   93 procdecl: ID args '=' DO '{' stmts '}'
   94         | ID args '=' DO '{' stmts '}' where_exp
   95         | ID '=' DO '{' stmts '}'
   96         | ID '=' DO '{' stmts '}' where_exp
   97         | ID error '=' '{' stmts '}'

   98 stmts: stmt
   99      | stmt stmts

  100 stmt: id_list LARROW expr ';'
  101     | id_list LARROW while_expr
  102     | id_list LARROW io_stmt ';'
  103     | yieldexpr ';'
  104     | while_expr
  105     | io_stmt ';'
  106     | error ';'

  107 io_stmt: READINT
  108        | READFLOAT
  109        | READBOOL
  110        | PRINT expr

  111 while_expr: WHILE '(' expr ')' '{' stmts '}'


